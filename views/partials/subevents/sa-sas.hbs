          <div class="content left aligned">
            <div class="header">Anirudh Varma</div>
            <div class="description">
              <button class="ui button small primary right floated" onClick="$('#sa-sas-anirudh-verma').modal('show')">Read Bio</button>
            </div>
          </div>
        </div>
      </div>
      <div class="column">
        <div class="ui card fluid">
          <div class="image">
            <img src="https://atkhrfnsco.cloudimg.io/v7/snsartsfestival.in/img/judges/sa-sas-anand-chauhan.png?w=400&h=400">
          </div>
          <div class="content left aligned">
            <div class="header">Anand Kishore Chauhan</div>
            <div class="description">
              <button class="ui button small primary right floated" onClick="$('#sa-sas-anand-chauhan').modal('show')">Read Bio</button>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
