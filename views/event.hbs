<style>
	body {
		margin: 0;
	}

	.v-header {
		height: 60vh;
		display: flex;
		align-items: center;
	}

	.h-container {
		max-width: 960px;
		padding-left: 1rem;
		padding-right: 1rem;
		margin: auto;
		{{!-- margin-top: 0; --}}
		margin-top: -54px;
		text-align: center;
	}

	.photo-wrap {
		position: absolute;
		{{!-- top: 54px; --}}
		top: 0px;
		left: 0;
		width: 100%;
		height: 60vh;
		overflow: hidden;
		{{headerGradient}}
	}

	.photo-wrap img {
		width: auto;
		display: block;
		position: absolute;
		left: 50%;
		top: 50%;
		transform: translate(-50%, -50%);
		z-index: 1;
	}

	@media screen and (max-aspect-ratio: 6/10) {
		.photo-wrap img {
			height: 100%;
		}
	}

	@media screen and (min-aspect-ratio: 6/10) {
		.photo-wrap img {
			width: 100%;
		}
	}

	.header-overlay {
		height: 60vh;
		position: absolute;
		{{!-- top: 54px; --}}
		top: 0px;
		left: 0;
		width: 100vw;
		z-index: 1;
		background: black;
		opacity: 0;
	}

	.header-content {
		z-index: 2;
		width: 100%;
	}

	.header-content img {
		margin-bottom: 20px;
	}

	.header-content h1,
	.header-content h4 {
		margin-bottom: 0;
	}

	.hero h1,
	.hero h4 {
		color: white!important;
	}

	.hero h1 {
		font-size: 4em !important;
	}


	#contact {
		padding-top: 3em;
		padding-bottom: 3em;
	}

	#content>div {
		padding-top: 5em;
		padding-bottom: 5em;
	}

	#contact a,
	#dev a {
		color: #46a3ff;
	}

	@media (max-width: 960px) {
		.h-container {
			padding-right: 3rem;
			padding-left: 3rem;
		}
	}

	/* @media (max-width: 576px) {
  .v-header, .photo-wrap, .header-overlay {
    height: 70vh;
  }
  .photo-wrap img {
    height: 100%!important;
  }
} */

